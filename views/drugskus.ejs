<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <!-- DrugSKU -->
  <div id="drugskus" ng-controller="DrugSKUController as drugSKUCntrl">
    <page-with-tiles 
      page-header="SKU препаратов" 
      add-button-caption="Добавить SKU" 
      model-meta="drugSKUCntrl.meta" 
      items="drugSKUCntrl.drugSKUs" 
      loaded="drugSKUCntrl.loaded" 
      tile-class="panel-primary" 
      tile-icon="fa fa-flask fa-5x" 
      field-for-tile-caption="name" 
      field-for-tile-r-row-1="brand.name" 
      create-id="drugsku_add" 
      create-caption="Новый SKU" 
      before-create-params="drugSKUCntrl.params" 
      modal-create-template="/templates/modal/drugSKU.html" 
      update-id="drugsku_upd" 
      update-caption="Обновление:" 
      field-for-update-caption="name" 
      modal-update-template="/templates/modal/drugSKU.html">
    </page-with-tiles>
  </div>
  <!-- ./DrugSKU -->
</div>