<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <!-- Agent -->
  <div id="agents" ng-controller="AgentController as agentCntrl">
    <page-with-tiles 
      page-header="Представители"
      add-button-caption="Добавить представителя"
      model-meta="agentCntrl.meta"
      items="agentCntrl.agents"
      loaded="agentCntrl.loaded"
      tile-class="panel-primary"
      tile-icon="fa fa-user fa-5x"
      field-for-tile-caption="fullName"
      field-for-tile-r-row-1="project.fullName"
      create-id="agent_add"
      create-caption="Новый представитель"
      before-create-params="agentCntrl.params"
      modal-create-template="/templates/modal/agent.html"
      update-id="agent_upd"
      update-caption="Обновление:"
      field-for-update-caption="shortName"
      modal-update-template="/templates/modal/agent.html">
    </page-with-tiles>
  </div>
  <!-- ./Agent -->
</div>