<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <!-- ListedHospital -->
  <div  id="materials" ng-controller="ListedHospitalController as listedHospCntrl">
    <page-with-tiles 
      page-header="Больницы для списка выбора" 
      add-button-caption="Добавить больницу" 
      model-meta="listedHospCntrl.meta" 
      items="listedHospCntrl.hospitals" 
      loaded="listedHospCntrl.loaded" 
      tile-class="panel-red" 
      tile-icon="fa fa-h-square fa-5x" 
      field-for-tile-caption="name" 
      field-for-tile-r-row-1="address" 
      create-id="listedhospital_add" 
      create-caption="Новая больница в список выбора" 
      modal-create-template="/templates/modal/listedHospital.html" 
      update-id="listedhospital_upd" 
      update-caption="Обновление:" 
      field-for-update-caption="name" 
      modal-update-template="/templates/modal/listedHospital.html">
    </page-with-tiles>
  </div>
  <!-- ./ListedHospital -->
</div>